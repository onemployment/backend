{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec \"npx ts-node src/index.ts\" --watch src --ext ts,js,json",
    "dev:build": "docker compose up --build -d",
    "dev:start": "docker compose up -d",
    "dev:stop": "docker compose down",
    "dev:clean": "docker compose down && docker rmi backend redis:8-alpine",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --selectProjects unit",
    "test:unit:watch": "jest --watch --selectProjects unit",
    "test:int": "jest --selectProjects integration",
    "test:int:watch": "jest --watch --selectProjects integration",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:status": "prisma migrate status",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "docker:db:migrate": "docker compose exec backend npm run db:migrate:dev",
    "docker:db:status": "docker compose exec backend npm run db:migrate:status",
    "docker:db:studio": "docker compose exec backend npm run db:studio",
    "docker:db:generate": "docker compose exec backend npm run db:generate",
    "docker:db:push": "docker compose exec backend npm run db:push",
    "docker:shell": "docker compose exec backend sh",
    "docker:logs": "docker compose logs -f backend",
    "docker:lint": "docker compose exec backend npm run lint",
    "setup:db": "./scripts/setup-db.sh",
    "seed:db": "docker compose exec backend npx ts-node scripts/seed.ts",
    "setup": "npm run setup:db && npm run seed:db"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "pino": "^9.9.0",
    "redis": "^4.7.0",
    "uuid": "^11.1.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^11.5.1",
    "@testcontainers/redis": "^11.5.1",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/prettier": "^2.7.3",
    "@types/redis": "^4.0.10",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^6.15.0",
    "supertest": "^7.1.4",
    "testcontainers": "^11.5.1",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
